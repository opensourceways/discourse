// jsdoc doesn't accept paths starting with _ (which is the case on github runners)
// so we need to alter the default config
{
  "source": {
    "excludePattern": ""
  },
  "templates": {
    "default": {
      "includeDate": false
    }
  },
  "opts": {
    "template": "./node_modules/tidy-jsdoc",
    "prism-theme": "prism-custom",
    "encoding": "utf8",
    "recurse": true
  },
  "metadata": {
    "title": "Discourse"
  }
}
